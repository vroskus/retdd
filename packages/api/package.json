{
  "name": "retdd.api",
  "version": "1.0.0",
  "author": "Vilius Ro≈°kus <info@regattas.eu>",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "babel src -d dist --copy-files && flow-copy-source src dist",
    "start": "yarn start clean && yarn start:build && start:serve",
    "start:clean": "rm -Rf dist",
    "start:build": "babel src -d dist --copy-files",
    "start:serve": "node dist/index.js",
    "dev": "nodemon --exec babel-node -- src/index.js",
    "eslint": "eslint src test --fix",
    "test": "yarn test:run && yarn test:post",
    "test:run": "DEPLOYMENT_ENV=TEST jest --ci --colors",
    "test:post": "jest-coverage-thresholds-bumper",
    "test:report": "open coverage/lcov-report/index.html"
  },
  "dependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.19.3",
    "@babel/eslint-parser": "7.19.1",
    "@babel/node": "7.19.1",
    "@babel/plugin-transform-modules-commonjs": "7.18.6",
    "@babel/preset-env": "7.19.3",
    "@babel/preset-flow": "7.18.6",
    "@babel/register": "7.18.9",
    "body-parser": "1.20.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "express": "4.18.1",
    "flow-copy-source": "2.0.9",
    "lodash": "4.17.21",
    "utility.enums": "^1.0.0",
    "utility.helpers": "^1.0.0",
    "utility.types": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "8.24.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sort": "2.4.0",
    "jest": "29.1.1",
    "jest-coverage-thresholds-bumper": "1.1.0",
    "nodemon": "2.0.20",
    "supertest": "6.2.4"
  },
  "jest": {
    "reporters": [
      "default"
    ],
    "coverageReporters": [
      "lcov",
      "json-summary",
      "text"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "statements": 96.87,
        "branches": 50,
        "lines": 100,
        "functions": 88.88
      }
    }
  }
}